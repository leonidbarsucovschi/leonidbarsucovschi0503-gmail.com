and ADM_USER.ID_USER = ADM_USER_SESSION.FK_USER and
FK_OPERATION_TYPE=16 and 
(substr(upper(ADM_USER.USER_LOGIN),1,3)not in ('PG_','DE_','CC_') and
substr(upper(ADM_USER.USER_LOGIN),1,4) not in ('MAI_'))
